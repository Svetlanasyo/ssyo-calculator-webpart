{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "calculator-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/calculator/CalculatorWebPart.js",
          "manifest": "./src/webparts/calculator/CalculatorWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery":"https://code.jquery.com/jquery-2.2.4.min.js",
    "jqueryui":"styles/jquery-ui-1.11.4.custom/jquery-ui.min.js"
  },
  "localizedResources": {
    "CalculatorWebPartStrings": "lib/webparts/calculator/loc/{locale}.js",
    "PropertyControlStrings": "./node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}